    <!-- Submit section -->

    <section id="submit">
        <div class="container px-4">
            <div class="row gx-4 justify-content-center">
                <div class="col-lg-8 text-center">
                    <br>
                    <h3 style="font-size: 35px;">Submit Your Scan</h3>
                    <h3>
                        <br>

                        <p class="lead">Please select the type of amputation to complete the process.</p>
                    </h3>
                    <br>
                    <form action="upload.php" method="POST" enctype="multipart/form-data">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="mb-3">
                                    <br>
                                    <label for="amputationType" class="form-label">Type of Amputation:</label>
                                    <select class="form-select" id="amputationType" name="amputationType" required>
                                        <option value="">Select Amputation Type</option>
                                        <option value="Leg">Leg</option>
                                        <option value="Arm">Arm</option>
                                        <option value="Hand">Hand</option>
                                        <option value="Foot">Foot</option>
                                        <option value="Other">Other</option>
                                    </select>
                                </div>
                                <div class="mb-3">
                                    <br>
                                    <br>
                                    <label for="imageUpload" class="form-label">Select Image:</label>
                                    <input class="form-control" type="file" id="imageUpload" name="imageUpload"
                                        accept="image/*" required>
                                </div>
                                <button class="btn btn-lg btn-primary" type="submit">Upload Image</button>
                            </div>
                            <div class="col-md-6 text-center mt-4 mt-md-0">
                                <img src="images/submitPage.png" alt="User Image" class="img-fluid"
                                    style="width: 1200px; height: auto;">
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>

    <!-- Results section -->
    <br><br><br><br><br>
    <section id="results">
        <div class="container px-4">
            <div class="row gx-4 justify-content-center">
                <div class="col-lg-8 order-lg-2 text-center">
                    <h2>Measurement Results</h2>
                    <p class="lead">Here are the measurement results based on the uploaded image:</p>
                    <div id="measurementResults" class="table-responsive">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Parameter</th>
                                    <th>Value</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Length</td>
                                    <td>Prosthetic Leg For Below Knee Amputees</td>
                                </tr>
                                <tr>
                                    <td>Breadth</td>
                                    <td>Stainless Steel/polyurethane</td>
                                </tr>
                                <tr>
                                    <td>Depth</td>
                                    <td>1.3 kg</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="col-lg-4 order-lg-1 text-center">
                    <img src="images/register.png" alt="Location Image" class="img-fluid"
                        style="width: 600px; height: auto;">
                </div>
            </div>
        </div>
        <br><br><br>
    </section>

    <section id="catalogue">
        <div class="container px-4">
            <div class="row gx-4 justify-content-center">
                <div class="col-lg-8 text-center">
                    <center></center>
                    
                    <p class="lead">Here are some suggested prosthetics based on the measurement results:</p>
                    <div id="prostheticSuggestions">

                        <div class="row">
                            

                                <% data.forEach(function(item) { %>
                                    <div class="col col-lg-2"></div>
                                    <div class="card border-primary">
                                        <img src="<% item.uri %>" class="card-img-top" alt="Prosthetic 1">
    
    
                                        <div class="card-body text-start d-flex flex-column">
                                            <h5 class="card-title"><% item.name %></h5>
                                            <p class="card-text">
                                                <strong>Structural Height:</strong> 18cm<br>
                                                <strong>Size-Range:</strong> 22cm ~ 27cm<br>
                                                <strong>Structural-Interval:</strong> 1cm<br>
                                                <strong>Material Type:</strong> Leather<br>
                                                <strong>Load Range:</strong> 110 kg<br>
                                                <strong>Color:</strong> Carbon Black
                                            </p>
                                            <button class="btn btn-primary mx-auto">Add to Cart</button>
                                        </div>
                                    </div>
                                  <% }); %>


                                




                                
                            </div>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <br><br><br><br>

    <footer class="py-5 bg-primary">
        <div class="container px-4">
            <div class="row align-items-center">
